{
  "name": "react-hooks",
  "version": "0.0.1",
  "private": true,
  "devDependencies": {
    "@types/react": "16.8.15",
    "@types/react-dom": "16.8.4",
    "@types/rebass": "3.0.3",
    "eslint": "5.16.0",
    "eslint-plugin-prettier": "3.0.1",
    "husky": "2.1.0",
    "lint-staged": "8.1.5",
    "prettier": "1.17.0",
    "react-scripts": "3.0.0",
    "tslint": "5.16.0"
  },
  "dependencies": {
    "@types/styled-components": "4.1.14",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "rebass": "3.1.0",
    "styled-components": "4.2.0",
    "typescript": "3.4.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "printWidth": 160
  },
  "eslintConfig": {
    "plugins": [
      "prettier"
    ],
    "extends": [
      "react-app",
      "prettier"
    ],
    "rules": {
      "prettier/prettier": "error",
      "no-unused-vars": "error",
      "react/prop-types": 0,
      "react/display-name": 0
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
